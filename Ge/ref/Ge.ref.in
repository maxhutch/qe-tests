&control
  calculation = 'scf'
  restart_mode = 'from_scratch'
  tstress = .true.
  tprnfor = .true.
  outdir = './raw'
  prefix = 'Ge-diamond'
  pseudo_dir = '../pseudo'
 /

 &system
  ibrav = 2
  celldm(1) = 10.885
  nat = 2
  ntyp = 1
  ecutwfc = 96
  ecutrho = 768
  occupations = 'smearing'
  smearing = 'fd'
  degauss = 6.336d-7
 /

 &electrons
  conv_thr = 1.d-12
  diagonalization = 'david'
  mixing_beta = 0.3
 /

 &srb
  use_srb = .false.
 /

